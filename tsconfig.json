{
    "compilerOptions": {
        "allowJs": true,
        "target": "es2017",
        "moduleResolution": "Node",
        "newLine": "CRLF",
        "outDir": "tsc-out",
        "baseUrl": "src/",
        "sourceMap": false,
        "strict": true,
    },
    "include": [
        "src/**/*"
    ],
    "exclude": [
        "node_modules"
    ],
}